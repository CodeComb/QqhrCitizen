@using QqhrCitizen.Models.ViewModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    Viewpager viwepager = ViewBag.ViwepagerShow;
    ViewBag.Title =viwepager .Title;
}

@using (Html.BeginForm("ViwepagerShow", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
    @Html.AntiForgerySID()
    <table class="detail-table">
    <tr>
        <td class="row-title">标题</td>
        <td>
            @viwepager.Title
        </td>
    </tr>
    <tr>
        <td class="row-title">
            描述
        </td>
        <td>
            @viwepager.Subtitle
        </td>
    </tr>
    <tr>
        <td class="row-title">
            Url
        </td>
        <td>
            @viwepager.Url
        </td>
    </tr>
    <tr>
        <td class="row-title">
            优先级
        </td>
        <td>
           @viwepager.Priority
        </td>
    </tr>
    <tr>
        <td class="row-title">
            图片
        </td>
        
    </tr>
</table>
}